[
  {
    "key": "ctrl+d",
    "command": "editor.action.deleteLines",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+k ctrl+t",
    "command": "-workbench.action.selectTheme"
  },
  {
    "key": "ctrl+k ctrl+t",
    "command": "editor.action.transformToTitlecase"
  },
  {
    "key": "ctrl+k ctrl+l",
    "command": "-editor.toggleFold",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "ctrl+k ctrl+l",
    "command": "editor.action.transformToLowercase"
  },
  {
    "key": "ctrl+k ctrl+u",
    "command": "-editor.action.removeCommentLine",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+k ctrl+u",
    "command": "editor.action.transformToUppercase"
  },
  {
    "key": "ctrl+shift+alt+z",
    "command": "latex-utilities.citeZotero"
  },
  {
    "key": "ctrl+shift+alt+a",
    "command": "editor.action.sortLinesAscending"
  },
  {
    "key": "ctrl+shift+alt+l",
    "command": "-workbench.action.quickchat.toggle",
    "when": "chatIsEnabled"
  },
  {
    "key": "ctrl+r ctrl+l",
    "command": "r.loadAll"
  },
  {
    "key": "ctrl+r ctrl+t",
    "command": "r.test"
  },
  {
    "key": "ctrl+r ctrl+c",
    "command": "r.check"
  },
  {
    "key": "ctrl+r ctrl+d",
    "command": "r.document"
  },
  {
    "key": "ctrl+q ctrl+r",
    "command": "quarto.renderProject"
  },
  {
    "key": "ctrl+r ctrl+i",
    "command": "editor.action.formatSelection",
    "when": "editorHasDocumentSelectionFormattingProvider && editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+g ctrl+d",
    "command": "github.copilot.completions.toggle"
  },
  {
    "key": "alt+\\",
    "command": "-editor.action.inlineSuggest.trigger",
    "when": "editorTextFocus && !editorHasSelection && !inlineSuggestionsVisible"
  },
  {
    "key": "ctrl+alt+i",
    "command": "editor.action.reindentselectedlines"
  },
  {
    "key": "ctrl+`",
    "command": "workbench.action.positronConsole.focusConsole"
  },
  {
    "key": "ctrl+k f",
    "command": "-workbench.action.positronConsole.focusConsole"
  }
]